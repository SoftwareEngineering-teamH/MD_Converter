package MD2HTML;

import static org.junit.Assert.*;

import org.junit.Test;

public class NodeTest {

	@Test
	public void testAccept() {
		
	}

	@Test
	public void testCreateString() 
	{
		Node test_node1 = new Node();
		Node test_node2 = new Node();
		test_node1.md_data = "abc";
		assertEquals(test_node1.md_data,test_node2.create("abc").md_data);
	}

	@Test
	public void testSetToken() {
		Node test_node1 = new Node();
		Node test_node2 = new Node();

	}

	@Test
	public void testSetStartTag() 
	{
		Node test_node1 = new Node();
		Node test_node2 = new Node();
		test_node1.md_data = "abc\n===\n123--\n# \n## \n### \n#### \n##### \n###### \n    123\n* \n1. \n----\n\n\n";
		
		assertEquals(test_node1.md_data,test_node2.create("abc\n===\n123--\n# \n## \n### \n#### \n##### \n###### \n    123\n* \n1. \n----\n\n\n").md_data);
	}

	@Test
	public void testSetEndTag() 
	{
		fail("Not yet implemented");
	}

}
