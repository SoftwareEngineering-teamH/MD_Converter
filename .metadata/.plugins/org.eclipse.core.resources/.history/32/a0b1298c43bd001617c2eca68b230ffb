package MD2HTML;

import static org.junit.Assert.*;

import org.junit.Test;

public class NodeTest {

	@Test
	public void testCreateString() {
		Node test_node = new Node();
		test_node.md_data = "abc";
		assertEquals(test_node.create("abc"),test_node);
	}

	@Test
	public void testCreateStringArray() {
		Node test_node = new Node();
		test_node.md_data_arr[4] = "abcd";
		assertEquals(test_node.create("abcd"),test_node);
	}

	@Test
	public void testSetToken() {
		fail("Not yet implemented");
	}

	@Test
	public void testSetStartTag() {
		fail("Not yet implemented");
	}

	@Test
	public void testSetEndTag() {
		fail("Not yet implemented");
	}

}
