
public class ItemList extends Node implements MDElement
{
	public Node create(String data)
	{
		this.md_data = data;
		
		// list ���� �ڿ� �����̽� �� ĭ ���� ��� ���� �ۼ��Ѵٰ� ����
		if(md_data.startsWith("* ") || md_data.startsWith("+ ") || md_data.startsWith("- "))
			this.node_type = 4;
		else
			this.node_type = 44;
		
		return this;
	}
}
